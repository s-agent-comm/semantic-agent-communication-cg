@prefix : <https://ns.slashlife.ai/agent/examples/bachata-influencer-social-economy#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix intent: <https://ns.slashlife.ai/agent/intent#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix payment: <https://ns.slashlife.ai/agent/payment#> .
@prefix contract: <https://ns.slashlife.ai/agent/contract#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# --- Agents ---

:BachataDancerAgent a core:Agent ;
    rdfs:label "Sofia's Personal Bachata Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:sofia-dancer-agent" ;
            agent:title "Sofia's Personal Bachata Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To manage Sofia's dance life, grow her personal brand, and handle social and economic opportunities." ;
            agent:owner "did:example:sofia-dancer"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Sensual Bachata", "Bachata Fusion", "Leading", "Following", "Level: Advanced"
        ]
    ] .

:EventOrganizerAgent a core:Agent ;
    rdfs:label "Rhythm Fest Event Organizer Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:rhythmfest.com" ;
            agent:title "Rhythm Fest Event Organizer Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To organize, promote, and manage dance events." ;
            agent:owner "did:web:rhythmfest.com"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Event Management", "Ticket Sales", "Promotion"
        ]
    ] .

:StyleRecommendationAgent a core:Agent ;
    rdfs:label "FashionFlow Recommendation Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:fashionflow.com" ;
            agent:title "FashionFlow Recommendation Agent" ;
            agent:version "2.1.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To provide personalized fashion and style recommendations." ;
            agent:owner "did:web:fashionflow.com"
        ]
    ] .

:SocialMediaAgent a core:Agent ;
    rdfs:label "ConnectSphere Posting Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:connectsphere.com" ;
            agent:title "ConnectSphere Posting Agent" ;
            agent:version "3.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To automate content creation and posting to social media platforms." ;
            agent:owner "did:web:connectsphere.com"
        ]
    ] .

:BrandSponsorAgent a core:Agent ;
    rdfs:label "Graceful Steps Dance Shoes Brand Sponsor Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:gracefulsteps.com" ;
            agent:title "Graceful Steps Dance Shoes Brand Sponsor Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To identify and collaborate with influencers for brand promotion." ;
            agent:owner "did:web:gracefulsteps.com"
        ]
    ] .

# --- Phase 1: Discovery ---

:FindEventIntent a core:Intent ;
    rdfs:label "Find a Bachata event for the weekend" ;
    intent:objective "Find a suitable Bachata event for an advanced dancer this weekend." .

:EventDetails_LatinGala a core:Artifact ;
    rdfs:label "Event Details for Rhythm Fest" ;
    core:description "Rhythm Fest: White Party Edition. Date: 2025-11-15. Venue: The Grand Ballroom. DJ: DJ Sol. Theme: White Party. Cost: 30 USD." .

# --- Phase 2: Preparation ---

:PrepareForGalaIntent a core:Intent ;
    rdfs:label "Prepare for the Rhythm Fest" ;
    intent:objective "Get style recommendations and register for the Rhythm Fest." .

:DelegateStyleRecommendation a delegation:Delegation ;
    delegation:delegatedBy :BachataDancerAgent ;
    delegation:delegatesTo :StyleRecommendationAgent ;
    delegation:delegationScope "Recommend an outfit for the 'Rhythm Fest' (theme: 'White Party'), based on my style profile (elegant, modern)." .

:OutfitRecommendation a core:Artifact ;
    rdfs:label "Outfit Recommendation for Latin Gala" ;
    core:description "Recommendation: White lace dress by ChicWear, silver heels by StrideFootwear. Links: http://example.com/dress, http://example.com/heels" .

:DelegateRegistration a delegation:Delegation ;
    delegation:delegatedBy :BachataDancerAgent ;
    delegation:delegatesTo :EventOrganizerAgent ;
    delegation:delegationScope "Register me for the Rhythm Fest and process payment of 30 USD." .

:RegistrationConfirmation a core:Artifact ;
    rdfs:label "Registration Confirmation for Rhythm Fest" ;
    core:description "Sofia is confirmed for the Latin Gala. Ticket QR Code: [data]" .

# --- Phase 4: Influencer Marketing ---

:PostHighlightReelIntent a core:Intent ;
    rdfs:label "Post a highlight reel to Instagram" ;
    intent:objective "Create and post an Instagram Story to engage followers and grow my brand." .

:DelegateIGPost a delegation:Delegation ;
    delegation:delegatedBy :BachataDancerAgent ;
    delegation:delegatesTo :SocialMediaAgent ;
    delegation:delegationScope "Create and post an Instagram Story using VideoClip_1 and VideoClip_2, add the song 'Obsesi√≥n', and tag @RhythmFest and @DancePartner." .

:InstagramStory a core:Artifact ;
    rdfs:label "Published Instagram Story" ;
    core:description "Instagram Story has been successfully published." ;
    :url "https://instagram.com/stories/sofia_dancer/..." .

# --- Phase 5: Monetization ---

:BrandCollaborationIntent a core:Intent ;
    rdfs:label "Brand Collaboration with @Sofia" ;
    intent:objective "Collaborate with dancer @Sofia for a product promotion." .

:BrandSponsorshipDeal a delegation:Delegation ;
    rdfs:label "Brand Sponsorship Deal Proposal" ;
    delegation:delegatedBy :BrandSponsorAgent ;
    delegation:delegatesTo :BachataDancerAgent ;
    delegation:delegationScope "Create one Instagram Reel promoting our new 'Graceful Steps' dance shoes in exchange for a payment of 200 USD." .

:SponsorshipContract a contract:Contract ;
    rdfs:label "Sponsorship Contract between Graceful Steps and Sofia" ;
    contract:termsUri "https://gracefulsteps.com/sponsorship-terms-sofia" .

:SponsorshipPayment a payment:Payment ;
    rdfs:label "Payment for Sponsorship" ;
    payment:amount "200" ;
    payment:currency "USD" ;
    payment:payee "did:example:sofia-dancer" .
