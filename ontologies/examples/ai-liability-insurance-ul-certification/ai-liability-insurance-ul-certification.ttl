@prefix : <https://ns.slashlife.ai/agent/examples/ai-liability-insurance-ul-certification#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix intent: <https://ns.slashlife.ai/agent/intent#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix execution-context: <https://ns.slashlife.ai/agent/execution-context#> .
@prefix ledger: <https://ns.slashlife.ai/agent/ledger#> .
@prefix payment: <https://ns.slashlife.ai/agent/payment#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# --- Agents ---

:Surgical_Robot_Agent a core:Agent ;
    rdfs:label "Autonomous Surgical Robot #SR-9001" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:surgical-robot-SN-9001" ;
            agent:title "Autonomous Surgical Robot #SR-9001" ;
            agent:version "4.2.1"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:owner "did:example:metro-health-hospital"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Perform 0.1mm precision cutting", "Suture tissue", "Laser ablation"
        ]
    ] .

:UL_Certification_Agent a core:Agent ;
    rdfs:label "GlobalCert Certification Agent for AI Systems" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:globalcert.com" ;
            agent:title "GlobalCert Certification Agent for AI Systems" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To test and certify the safety and capabilities of AI agents against established standards." ;
            agent:owner "did:web:globalcert.com"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "AI Capability Verification", "Safety Constraint Testing", "Verifiable Certificate Issuance"
        ]
    ] .

:Insurer_Underwriting_Agent a core:Agent ;
    rdfs:label "Insurer Underwriting Agent for AI Liability" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:ai-assurance.com" ;
            agent:title "Insurer Underwriting Agent for AI Liability" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:owner "did:web:ai-assurance.com"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Risk Assessment", "Policy Underwriting", "Verifiable Credential Validation"
        ]
    ] .

:Insurer_Claims_Adjudication_Agent a core:Agent ;
    rdfs:label "Insurer Claims Adjudication Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:web:ai-assurance.com:claims" ;
            agent:title "Insurer Claims Adjudication Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To adjudicate insurance claims by analyzing immutable operational data from insured AI agents." ;
            agent:owner "did:web:ai-assurance.com"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Ledger Analysis", "Trace Event Verification", "Liability Determination"
        ]
    ] .

:Surgeon_Agent a core:Agent ;
    rdfs:label "Dr. Alice Turing, Chief Surgeon" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:surgeon-alice-turing" ;
            agent:title "Dr. Alice Turing, Chief Surgeon" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To supervise and direct surgical procedures, delegating specific tasks to autonomous systems." ;
            agent:owner "did:example:city-hospital"
        ]
    ] .

# --- Phase 1: Certification ---

:CertifyRobotAction a core:Action ;
    rdfs:label "Certify Surgical Robot #SR-9001" ;
    core:contextOf :GlobalCert_Certification_Agent ;
    core:producesArtifact :GlobalCert_Safety_Certificate .

:UL_Safety_Certificate a core:Artifact ;
    rdfs:label "GlobalCert Safety Certificate for Surgical Robot #SR-9001" ;
    core:description "Certifies that the agent 'did:example:surgical-robot-SN-9001' has been tested and conforms to safety standard GlobalCert-AI-9000. Certified capabilities: 'Perform 0.1mm precision cutting', 'Suture tissue', 'Laser ablation'. Operational constraints: Max speed 1mm/s." ;
    security:securityBinding [
        a security:SecurityBinding ;
        security:cryptographicBinding "JWS" ;
        security:trustModel "GlobalCert Certified Issuer" ;
        security:signedBy :UL_Certification_Agent
    ] .

# --- Phase 2: Insurance ---

:InsureRobotIntent a core:Intent ;
    rdfs:label "Intent to Insure Surgical Robot #SR-9001" ;
    intent:objective "To obtain comprehensive liability insurance for Surgical Robot #SR-9001, based on its UL certification." .

:IssuePolicyAction a core:Action ;
    rdfs:label "Issue Insurance Policy for Surgical Robot #SR-9001" ;
    core:contextOf :Insurer_Underwriting_Agent ;
    core:producesArtifact :Insurance_Policy .

:Insurance_Policy a core:Artifact ;
    rdfs:label "AI Liability Insurance Policy #AI-LIAB-123" ;
    core:description "Policy covering operations of 'did:example:surgical-robot-SN-9001'. Coverage is contingent upon operations being within the scope of the referenced UL Safety Certificate." ;
    :referencesCertificate :GlobalCert_Safety_Certificate ;
    security:securityBinding [
        a security:SecurityBinding ;
        security:cryptographicBinding "JWS" ;
        security:trustModel "Insurer Certified Issuer" ;
        security:signedBy :Insurer_Underwriting_Agent
    ] .

# --- Phase 3: Operation ---

:DelegateSurgicalTask a delegation:Delegation ;
    delegation:delegatedBy :Surgeon_Agent ;
    delegation:delegatesTo :Surgical_Robot_Agent ;
    delegation:delegationScope "Perform a 5cm linear incision at coordinates (X,Y,Z), depth 2mm, speed <= 1mm/s." .

:PerformIncisionAction a core:Action ;
    rdfs:label "Perform Surgical Incision" ;
    core:contextOf :DelegateSurgicalTask ;
    core:producesArtifact :SurgicalOutcome .

:SurgicalOutcome a core:Artifact ;
    rdfs:label "Surgical Outcome for Incision" ;
    core:description "Incision performed. Deviation of +1mm from specified depth detected." .

# --- Phase 4 & 5: Claim and Adjudication ---

:FileClaimIntent a core:Intent ;
    rdfs:label "Intent to File Claim for Incident #INC-5678" ;
    intent:objective "To file an insurance claim for patient injury resulting from operational deviation during surgery." .

:AnalyzeIncidentTraceAction a core:Action ;
    rdfs:label "Analyze Incident Trace from Ledger" ;
    core:contextOf :Insurer_Claims_Adjudication_Agent ;
    :accessesLedger <https://ledger.metro-health-hospital.com/robot-sr-9001> ;
    core:producesArtifact :AdjudicationReport .

:AdjudicationReport a core:Artifact ;
    rdfs:label "Adjudication Report for Incident #INC-5678" ;
    core:description "Analysis of immutable trace data confirms the surgical robot deviated from its delegated task scope while operating within its certified capabilities. The incident is covered by policy #AI-LIAB-123." .

:SettleClaimAction a payment:Payment ;
    rdfs:label "Settle Insurance Claim" ;
    core:contextOf :Insurer_Claims_Adjudication_Agent ;
    payment:amount "500000" ;
    payment:currency "USD" ;
    payment:payee "did:example:city-hospital" .
