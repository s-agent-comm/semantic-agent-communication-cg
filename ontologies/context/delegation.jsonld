{
  "@context": {
    "@vocab": "https://ns.slashlife.ai/agent/delegation#",
    "core": "https://ns.slashlife.ai/agent/core#",
    "cap": "https://ns.slashlife.ai/agent/capability#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",

    "id": "@id",
    "type": "@type",

    "Delegation": "core:Delegation",
    "delegationId": "del:delegationId",

    "grantor": { "@id": "del:delegatedBy", "@type": "@id" },
    "grantee": { "@id": "del:delegatesTo", "@type": "@id" },

    "scope": { "@id": "del:delegationScope", "@type": "@id" },
    "DelegationScope": "del:DelegationScope",

    "capabilities": { "@id": "del:allowedCapability", "@type": "@id" },
    "intent": { "@id": "del:intentConstraint", "@type": "@id" },
    
    "validFrom": { "@id": "del:validFrom", "@type": "xsd:dateTime" },
    "validUntil": { "@id": "del:validUntil", "@type": "xsd:dateTime" },

    "delegationChain": { "@id": "del:hasDelegation", "@type": "@id" },
    "DelegationChain": "del:DelegationChain",

    "accountableFor": { "@id": "del:accountableFor", "@type": "@id" },
    "recordedIn": { "@id": "del:recordedIn", "@type": "@id" },

    "constraints": { "@id": "del:executionConstraint", "@type": "@id" },
    "ExecutionConstraint": "del:ExecutionConstraint",

    "note": "del:note"
  }
}
